# Local Postgres defaults for MED13 Resource Library
# Copy to .env.postgres (gitignored) and adjust as needed.

# Docker container configuration
MED13_POSTGRES_DB=med13_dev
MED13_POSTGRES_USER=med13_dev
MED13_POSTGRES_PASSWORD=med13_dev_password
MED13_POSTGRES_PORT=5432
MED13_POSTGRES_HOST=localhost

# SQLAlchemy connection strings
DATABASE_URL=postgresql+psycopg2://med13_dev:med13_dev_password@localhost:5432/med13_dev
ASYNC_DATABASE_URL=postgresql+asyncpg://med13_dev:med13_dev_password@localhost:5432/med13_dev
ALEMBIC_DATABASE_URL=postgresql+psycopg2://med13_dev:med13_dev_password@localhost:5432/med13_dev
